{
  "name": "book-library",
  "version": "1.0.0",
  "description": "Powered by OKTAY KÖSE(okoseisback@gmail.com)",
  "main": "dist/src/server.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "env:copy": "cp .example.env .env",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register src/server.ts | pino-pretty",
    "clean": "rm -rf dist/",
    "build": "cross-env NODE_ENV=production tsc && tsc-alias",
    "start": "node dist/src/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "test:cov": "npm run test --coverage --watchAll",
    "test:watch": "npm run  test --watchAll",
    "prisma:gen": "prisma generate",
    "prisma:db:migrate:reset": "prisma migrate reset",
    "prisma:db:seed": "ts-node-dev prisma/seed.ts",
    "swagger-gen": "node swagger.js"
  },
  "keywords": [],
  "author": "Oktay KÖSE(okoseisback@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.12.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.12.0",
    "swagger-autogen": "^2.23.7",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  }
}
